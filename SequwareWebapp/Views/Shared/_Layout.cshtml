    function copyAllcases() {
    debugger;
    var Resultdata;
    var url = window.location.pathname.split("/");
    var controller = url[1];
    var apiurl = "";
    var calltype = "GET";
    if (controller == "Cases") {
    apiurl = 'api/_CustomerRequest/GetMainRequest?CompanyID=' + $('#hdncompanyID').val();
